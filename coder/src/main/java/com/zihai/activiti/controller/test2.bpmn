    <sequenceFlow id="flow5" sourceRef="usertask2" targetRef="endevent2"></sequenceFlow>
    <userTask id="usertask3" name="用户3" activiti:candidateUsers="user3"></userTask>
    <sequenceFlow id="flow6" sourceRef="startevent1" targetRef="usertask3"></sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="usertask3" targetRef="subprocess1"></sequenceFlow>
